def my_genrule(
    name,
    out,
    visibility = [],
  ):
  genrule(
    name = name,
    srcs = [
      'foo.txt',
      'bar.txt',
    ],
    bash = 'cat $SRCS > $OUT',
    out = out,
    deps = [
      '//fake:rule',
    ],
    visibility = visibility,
  )

my_genrule(
  name = 'example',
  out = 'baz.txt',
  visibility = [
    'PUBLIC',
  ],
)

keystore(
  name = 'debug_keystore',
  store = 'debug.keystore',
  properties = 'debug.keystore.properties',
)
